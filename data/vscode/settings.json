{
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[handlebars]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[sql]": {
    "editor.defaultFormatter": "i-dont-want-an-sql-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "auto-close-tag.disableOnLanguage": [
    "typescript"
  ],
  "css.validate": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.fontFamily": "Source Code Pro",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment"
        ],
        "settings": {
          "foreground": "#e0efd9ba"
        }
      },
      {
        "scope": [
          "punctuation.definition.comment.json",
          "punctuation.definition.comment.js",
          "punctuation.definition.comment.ts"
        ],
        "settings": {
          "foreground": "#fff96933",
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": [
          "keyword.operator.logical.ts",
          "keyword.operator.logical.js",
          "keyword.operator.ternary.ts",
          "keyword.operator.ternary.js",
          "keyword.control.flow.ts",
          "keyword.control.flow.js",
        ],
        "settings": {
          "foreground": "#FF7FBE",
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "string.regexp.ts",
          "string.regexp.js",
        ],
        "settings": {
          "foreground": "#FF0000",
          "fontStyle": "",
        }
      }
    ]
  },
  "eslint.format.enable": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "git.closeDiffOnOperation": true,
  "git.mergeEditor": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "vue": false
  },
  "html.format.endWithNewline": true,
  "html.format.indentHandlebars": true,
  "javascript.format.enable": true,
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.styles": [
    ".vscode/markdown.css"
  ],
  "prettier.arrowParens": "avoid",
  "prettier.requireConfig": true,
  "scss.validate": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.automationProfile.osx": {
    "path": "/bin/bash"
  },
  "terminal.integrated.fontFamily": "monospace",
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    {
      "color": "yellow",
      "backgroundColor": "#ffff0022 !important",
      "text": "TODO"
    },
    {
      "color": "red",
      "backgroundColor": "#ff000011 !important",
      "text": "WIP",
    },
  ],
  "typescript.autoClosingTags": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "workbench.colorCustomizations": {
    "merge.currentHeaderBackground": "#AAFFFF01",
    "merge.incomingHeaderBackground": "#AAFFFF01",
    "editorOverviewRuler.commonContentForeground": "#00FFFF99",
    "editorOverviewRuler.currentContentForeground": "#00DDAA",
    "editorOverviewRuler.incomingContentForeground": "#00AADD",
    "scrollbar.shadow": "#00AADD22"
  },
  "workbench.colorTheme": "Tokyo Night",
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}